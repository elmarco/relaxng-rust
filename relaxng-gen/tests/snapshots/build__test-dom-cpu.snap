    where
        W: std::io::Write,
    {
        use quick_xml::events::{BytesEnd, BytesStart, BytesText, Event};
        match self {
            Self::Yes => {
                writer.write_event(Event::Text(BytesText::new("yes")))?;
            }
            Self::No => {
                writer.write_event(Event::Text(BytesText::new("no")))?;
            }
        }
        Ok(())
    }
