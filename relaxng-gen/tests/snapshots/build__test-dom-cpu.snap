//!`mode` module
use crate::{Error, Result, ToXml};
use std::str::FromStr;
use crate::r#enum::Enum;
use crate::vir_yes_no::VirYesNo;
///`<mode/>` element
#[derive(Debug, Clone, PartialEq, Eq, Hash)]
pub struct Maximum {
    pub supported: VirYesNo,
    pub r_enums: Vec<Enum>,
}
impl ToXml for Maximum {
    type Builder = MaximumBuilder;
    fn to_xml<W>(&self, writer: &mut quick_xml::Writer<W>) -> Result<()>
    where
        W: std::io::Write,
    {
        use quick_xml::events::{Event, BytesStart, BytesEnd, BytesText};
        let mut start = BytesStart::new("mode");
        start
            .push_attribute((
                &b"name"[..],
                quick_xml::escape::escape("maximum").as_bytes(),
            ));
        {
            let elem = &self.supported;
            start
                .push_attribute((
                    &b"supported"[..],
                    quick_xml::escape::escape(&elem.to_string()).as_bytes(),
                ));
        }
        let empty = {
            true
                && ({
                    let elem = &self.r_enums;
                    elem.iter().all(|elem| false)
                })
        };
        if empty {
            writer.write_event(Event::Empty(start))?;
        } else {
            writer.write_event(Event::Start(start))?;
            {
                let elem = &self.r_enums;
                for elem in elem {
                    elem.to_xml(writer)?;
                }
            }
            writer.write_event(Event::End(BytesEnd::new("mode")))?;
        }
        Ok(())
    }
}
impl Maximum {
    pub fn from_xml(node: &roxmltree::Node) -> Result<Self> {
        let mut builder = Self::builder();
        if matches!(node.attribute("name"), Some("maximum")) {
            builder.name()?;
        }
        if let Some(val) = node.attribute("supported") {
            builder.supported(VirYesNo::from_str(val)?)?;
        }
        for child in node.children() {
            match child.tag_name().name() {
                "enum" if Enum::from_xml(&child).is_ok() => {
                    builder.r_enum(Enum::from_xml(&child)?)?;
                }
                _ => continue,
            }
        }
        if let Some(val) = node.text() {
            match val {
                _ => {}
            }
        }
        builder.build()
    }
}
impl core::fmt::Display for Maximum {
    fn fmt(&self, f: &mut core::fmt::Formatter<'_>) -> std::fmt::Result {
        let mut writer = quick_xml::Writer::new_with_indent(
            std::io::Cursor::new(Vec::new()),
            b' ',
            2,
        );
        self.to_xml(&mut writer).expect("Failed to generate XML");
        let res = writer.into_inner().into_inner();
        let str = String::from_utf8(res).expect("Failed to convert bytes to string");
        f.write_str(&str)
    }
}
///`<mode/>` builder
#[derive(Default, Debug, Clone, PartialEq, Eq, Hash)]
pub struct MaximumBuilder {
    pub name: Option<()>,
    pub supported: Option<VirYesNo>,
    pub r_enums: Vec<Enum>,
}
impl MaximumBuilder {
    pub fn name(&mut self) -> Result<&mut Self> {
        self.name = Some(());
        Ok(self)
    }
    pub fn supported<T>(&mut self, supported: T) -> Result<&mut Self>
    where
        T: TryInto<VirYesNo>,
        Error: From<<T as TryInto<VirYesNo>>::Error>,
    {
        self.supported = Some(supported.try_into()?);
        Ok(self)
    }
    pub fn r_enum<T>(&mut self, r_enum: T) -> Result<&mut Self>
    where
        T: TryInto<Enum>,
        Error: From<<T as TryInto<Enum>>::Error>,
    {
        self.r_enums.push(r_enum.try_into()?);
        Ok(self)
    }
    /**
            Attempt to build [`Maximum`].

            If the element is not valid, an error is returned.
            */
    pub fn build(&self) -> Result<Maximum> {
        let name = Clone::clone(
