            if let Some(elem) = elem {
                xml_start
                    .push_attribute((
                        &b"ats"[..],
                        quick_xml::escape::escape(&elem.to_string()).as_bytes(),
                    ));
            }
        }
        let empty = { true };
        if empty {
            writer.write_event(Event::Empty(xml_start))?;
        } else {
            writer.write_event(Event::Start(xml_start))?;
            writer.write_event(Event::End(BytesEnd::new("domain")))?;
        }
        Ok(())
    }
}
