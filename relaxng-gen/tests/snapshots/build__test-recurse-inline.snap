pub struct InlineBuilder {
    value: Option<String>,
    code: Option<Code>,
    em: Option<Em>,
}
impl InlineBuilder {
    pub fn value<T>(&mut self, value: T) -> Result<&mut Self>
    where
        T: TryInto<String>,
        Error: From<<T as TryInto<String>>::Error>,
    {
        self.value = Some(value.try_into()?);
        Ok(self)
    }
    pub fn code<T>(&mut self, code: T) -> Result<&mut Self>
    where
        T: TryInto<Code>,
        Error: From<<T as TryInto<Code>>::Error>,
    {
        self.code = Some(code.try_into()?);
        Ok(self)
    }
    pub fn em<T>(&mut self, em: T) -> Result<&mut Self>
    where
        T: TryInto<Em>,
        Error: From<<T as TryInto<Em>>::Error>,
    {
        self.em = Some(em.try_into()?);
        Ok(self)
    }
    pub fn build(&self) -> Result<Inline> {
        if self.value.is_some() {
            return Ok(Inline::Value(Clone::clone(&self.value.as_ref().unwrap())));
        }
        if self.code.is_some() {
            return Ok(Inline::Code(Clone::clone(&self.code.as_ref().unwrap())));
        }
        if self.em.is_some() {
            return Ok(Inline::Em(Clone::clone(&self.em.as_ref().unwrap())));
        }
        Err(Error::BuilderVariant("Inline"))
    }
}

