use crate::{Error, Result, ToXml};
use std::str::FromStr;
pub mod choice1;
use choice1::Choice1;
pub mod ready_choice;
use ready_choice::ReadyChoice;
///`<mirror/>` element
#[derive(Debug, Clone, PartialEq, Eq, Hash)]
pub struct Mirror {
    pub choice1: Choice1,
    pub ready: Option<ReadyChoice>,
}
impl ToXml for Mirror {
    type Builder = MirrorBuilder;
    fn to_xml<W>(&self, writer: &mut quick_xml::Writer<W>) -> Result<()>
    where
        W: std::io::Write,
    {
        use quick_xml::events::{Event, BytesStart, BytesEnd, BytesText};
        let mut xml_start = BytesStart::new("mirror");
        {
            let elem = &self.choice1;
            elem.to_xml_attr(&mut xml_start)?;
        }
        {
            let elem = &self.ready;
            if let Some(elem) = elem {
                xml_start
                    .push_attribute((
                        &b"ready"[..],
                        quick_xml::escape::escape(&elem.to_string()).as_bytes(),
                    ));
            }
        }
        let empty = {
            true
                && ({
                    let elem = &self.choice1;
                    elem.is_empty()
                })
        };
        if empty {
            writer.write_event(Event::Empty(xml_start))?;
        } else {
            writer.write_event(Event::Start(xml_start))?;
            {
                let elem = &self.choice1;
                elem.to_xml(writer)?;
            }
            writer.write_event(Event::End(BytesEnd::new("mirror")))?;
        }
        Ok(())
    }
}
impl Mirror {
    pub fn from_xml(node: &roxmltree::Node) -> Result<Self> {
        let mut builder = Self::builder();
        if let Some(val) = node.attribute("ready") {
            builder.ready(Some(ReadyChoice::from_str(val)?))?;
        }
        for child in node.children() {
            match child.tag_name().name() {
                _ => continue,
            }
        }
        if let Some(val) = node.text() {
            match val {
                _ => {}
            }
        }
        let child = &mut node.first_child();
        if let Ok(val) = Choice1::from_xml(node, child) {
            builder.choice1(val)?;
        }
        builder.build()
    }
}
impl core::fmt::Display for Mirror {
    fn fmt(&self, f: &mut core::fmt::Formatter<'_>) -> std::fmt::Result {
        let mut writer = quick_xml::Writer::new_with_indent(
            std::io::Cursor::new(Vec::new()),
            b' ',
            2,
        );
        self.to_xml(&mut writer).expect("Failed to generate XML");
        let res = writer.into_inner().into_inner();
        let str = String::from_utf8(res).expect("Failed to convert bytes to string");
        f.write_str(&str)
    }
}
///`<mirror/>` builder
#[derive(Default, Debug, Clone, PartialEq, Eq, Hash)]
pub struct MirrorBuilder {
    pub choice1: Option<Choice1>,
    pub ready: Option<ReadyChoice>,
}
impl MirrorBuilder {
    pub fn choice1<T>(&mut self, choice1: T) -> Result<&mut Self>
    where
        T: TryInto<Choice1>,
        Error: From<<T as TryInto<Choice1>>::Error>,
    {
        self.choice1 = Some(choice1.try_into()?);
        Ok(self)
    }
    pub fn ready<T>(&mut self, ready: Option<T>) -> Result<&mut Self>
    where
        T: TryInto<ReadyChoice>,
        Error: From<<T as TryInto<ReadyChoice>>::Error>,
    {
        self.ready = if let Some(ready) = ready {
            Some(ready.try_into()?)
        } else {
