    let xml = roxmltree::Document::parse(&xml).expect("Failed to parse XML");
    let root = xml.root().first_child().expect("No root element found");
    let res = test_input_bus::Input::from_xml(&root).unwrap();
    let res = res.to_string();
    println!("{}", res);
