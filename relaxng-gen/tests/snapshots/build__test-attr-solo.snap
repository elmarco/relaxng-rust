    where
        W: std::io::Write,
    {
        use quick_xml::events::{Event, BytesStart, BytesEnd, BytesText};
        let mut xml_start = BytesStart::new("tr");
        xml_start
            .push_attribute((
                &b"class"[..],
                quick_xml::escape::escape("card").as_bytes(),
